{% set opener = '' %}
{% set closer = '' %}
{% if type == 'tr' %}
 {% set opener = '<td>' %}
 {% set closer = '</td>' %}
{% endif %} 
<{{ type }} class="form-field">
	{{ opener|raw }}
	<label for="ai1ec_address"> {{ 'Address:' | __ }} </label> 
	{{ closer|raw }}
	{{ opener|raw }}
	<input
		type='text' id='ai1ec_address' name='ai1ec_address'
		value="{{ address }}" />
	{{ closer|raw }}
</{{ type }}>
{% if type == 'div' %}
<table>
{% endif %}
	<tr>
		<td><label for="ai1ec_google_map"> {{ 'Show Google Map' | __ }} </label>
		</td>
		<td><input type="checkbox" value="1" name="ai1ec_google_map"
			id="ai1ec_google_map" checked></td>
	</tr>
{% if type == 'div' %}
</table>
{% endif %}
<{{ type }} class="form-field">
	{{ opener|raw }}
	<label for="ai1ec_longitude"> {{ 'Longitude' | __ }} </label> 
	{{ closer|raw }}
	{{ opener|raw }}
	<input
		type="text" class="longitude coordinates" name="ai1ec_longitude"
		id="ai1ec_longitude" class="ai1ec-form-control"
		value="{{ longitude }}">
	{{ closer|raw }}
</{{ type }}>
<{{ type }} class="form-field">
	{{ opener|raw }}
	<label for="ai1ec_latitude"> {{ 'Latitude' | __ }} </label>
	{{ closer|raw }}
	{{ opener|raw }}
	 <input
		type="text" class="latitude coordinates" name="ai1ec_latitude"
		id="ai1ec_latitude" class="ai1ec-form-control" value="{{ latitude }}">
	{{ closer|raw }}
</{{ type }} >
<{{ type }} class="form-field">
	{{ opener|raw }}
		<div>Google map</div>
	{{ closer|raw }}
	{{ opener|raw }}
	<div class="ai1ec_box_map" >
		<div id="ai1ec_map_canvas" style="height: 300px;width"></div>
	</div>
	{{ closer|raw }}
</{{ type }}>
<{{ type }} class="form-field">
	{{ opener|raw }}
	<label for="ai1ecv_venue-description"> {{ 'Description:' | __ }} </label>
	{{ closer|raw }}
	{{ opener|raw }}
	<textarea name='ai1ecv_venue-description'>{{ description }}</textarea>
	{{ closer|raw }}
</{{ type }}>

<{{ type }} class="form-field">
	{{ opener|raw }}
	<label for="ai1ecv_venue-contact-name"> {{ 'Contact name:' | __ }} </label>
	{{ closer|raw }}
	{{ opener|raw }}
	<input type='text' name='ai1ecv_venue-contact-name'
		value="{{ contact_name }}" />
	{{ closer|raw }}
</{{ type }}>

<{{ type }} class="form-field">
	{{ opener|raw }}
	<label for="ai1ecv_venue-phone"> {{ 'Phone:' | __ }} </label>
	{{ closer|raw }} 
	{{ opener|raw }}
	<input
		type='text' name='ai1ecv_venue-phone' value="{{ phone }}" />
	{{ closer|raw }}
</{{ type }}>

<{{ type }} class="form-field">
	{{ opener|raw }}
	<label for="ai1ecv_venue-email"> {{ 'Email:' | __ }} </label> 
	{{ closer|raw }}
	{{ opener|raw }}
	<input
		type='text' name='ai1ecv_venue-email' value="{{ email }}" />
	{{ closer|raw }}
</{{ type }}>

<{{ type }} class="form-field">
	{{ opener|raw }}
	<label for="ai1ecv_venue-url"> {{ 'External url:' | __ }} </label>
	{{ closer|raw }} 
	{{ opener|raw }}
	<input
		type='text' name='ai1ecv_venue-url' value="{{ url }}" />
	{{ closer|raw }}
	</{{ type }}>

	<{{ type }} class="form-field">
		{{ opener|raw }}
		<label for="ai1ecv_venue-capacity"> {{ 'Seating capacity:' | __ }} </label>
		{{ closer|raw }}
		{{ opener|raw }}
		<input type='text' name='ai1ecv_venue-capacity' value="{{ capacity }}" />
		{{ closer|raw }}
	</{{ type }}>

	<{{ type }} class="form-field">
		{{ opener|raw }}
		<label for="ai1ecv_venue-handicap"> {{ 'Handicap Accessible:' | __ }} </label>
		{{ closer|raw }}
		{{ opener|raw }}
		<input type='checkbox' name='ai1ecv_venue-handicap' value="1"
		{% if handicap %} checked="true" {% endif %} />
		{{ closer|raw }}
	</{{ type }} >

	<{{ type }} class="form-field">
		{{ opener|raw }}
		<label for="ai1ecv_venue-parking"> {{ 'Parking:' | __ }} </label>
		{{ closer|raw }}
		{{ opener|raw }}
		<textarea name='ai1ecv_venue-parking'>{{ parking }}</textarea>
		{{ closer|raw }}
	</{{ type }}>
	<style type="text/css">
.ai1ec-geo-ac-results-not-ready {
	visibility: hidden;
}

.ai1ec-geo-ac-results img {
	float: left;
	margin-right: 5px;
}

.ai1ec-geo-ac-results .ai1ec-dropdown-menu {
	display: block;
	width: 275px;
	background-color: #FFFFFF;
}

.ai1ec-geo-ac-results .ai1ec-dropdown-menu li {
	clear: left;
	color: #333333;
	cursor: pointer;
	margin: 2px 0;
	padding: 0 5px;
}

.ai1ec-geo-ac-results .ai1ec-dropdown-menu li:hover {
	color: #262626;
	background-color: #f5f5f5;
}

</style>
	<input type="hidden" name="ai1ec_city" id="ai1ec_city"
		value="{{ city }}"> 
	<input type="hidden" name="ai1ec_province"
		id="ai1ec_province" value="{{ province }}"> 
	<input type="hidden"
		name="ai1ec_postal_code" id="ai1ec_postal_code"
		value="{{ postal_code }}"> 
	<input type="hidden" name="ai1ec_country"
		id="ai1ec_country" value="{{ country }}">
	<script type="text/javascript">
	jQuery('.form-wrap h3').hide();
	jQuery('.wrap h2').html(jQuery('.wrap h2').html().replace('Category', 'Venue'));
	jQuery('#tag-description').parent().hide();
	jQuery('.column-description').hide();
	jQuery('#description').parent().parent().hide();
	jQuery(document).ready(function($){
		if ( $('#addtag .button-primary').length > 0 ) {
			$('#addtag .button-primary').val($('#addtag .button-primary').val().replace('Category', 'Venue'));
		}
		
	});
</script>
